{
    "name": "v-dem/queasy-config",
    "description": "Configuration classes (currently supports PHP, INI and JSON configs), part of Queasy PHP framework",
    "homepage": "https://github.com/v-dem/queasy-config/",
    "keywords": ["config", "ini", "json"],
    "license": "LGPL-3.0",
    "minimum-stability": "dev",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Vitaly Demyanenko",
            "email": "vitaly_demyanenko@yahoo.com"
        }
    ],
    "require": {
        "php": ">=5.3.0|>=7.0.0"
    },
    "require-dev": {
        "php": ">=5.6.0|>=7.0.0",
        "phpunit/phpunit": "~5.7"
    },
    "autoload":{
        "psr-4":{
            "queasy\\config\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "queasy\\config\\tests\\": "tests/"
        }
    }
}

